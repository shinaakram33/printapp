    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "stripe": "^9.1.0",
    "swagger-ui-express": "^4.3.0",
    "twilio": "^3.76.0",
    "uuid": "^8.3.2"
