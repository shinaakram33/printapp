    "@nestjs/swagger": "^5.2.1",
    "@sendgrid/mail": "^7.7.0",
    "aws-sdk": "^2.1112.0",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
